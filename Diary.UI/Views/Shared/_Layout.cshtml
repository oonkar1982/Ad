<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Diary.UI</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/perfect-scrollbar.css" />
    <link rel="stylesheet" href="~/css/prism.css" />
    <link href="~/lib/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">

</head>
<body>

    <div class="wrapper">
        <div class="Preloader-wrapper" style="display: none;"><div class="spinner"> <div class="bounce1"></div> <div class="bounce2"></div> <div class="bounce3"></div> </div> </div>
        <nav class="sidebar">
            <div class="sidebar-content ">
                <a class="sidebar-brand" href="dashboard.html">
                    <img src="">
                </a>
                <ul class="sidebar-nav ps-container ps-theme-default ps-active-y" style="height: 348px;" data-ps-id="74467d3a-1933-54d9-a104-a81940ede8d3">
                    <li class="sidebar-header">-- Main</li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" asp-controller="Home" asp-action="Index">
                            Dashboard 1
                        </a>
                    </li>
                    <li class="sidebar-item  ">
                        <a class="sidebar-link" asp-controller="Lawsuits" asp-action="Index">Lawsuits </a>
                    </li>
                    <li class="sidebar-item  ">
                        <a class="sidebar-link" asp-controller="Courts" asp-action="Index">Courts </a>
                    </li>
                    <li class="sidebar-item  ">
                        <a class="sidebar-link" asp-controller="SMS" asp-action="Index">SMS </a>
                    </li>



                </ul>
            </div>
        </nav>
        <!-- ======= Sidebar ======= -->


        <div class="main">
            <nav class="navbar navbar-expand navbar-light bg-white">
                <a class="sidebar-toggle d-flex mr-2">
                    <i class="hamburger align-self-center"></i>
                </a>
                <div class="navbar-collapse collapse">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <form class="form-inline d-none d-sm-inline-block">
                                <input class="form-control form-control-no-border mr-sm-2 searchinputbox" type="text" placeholder="Search projects..." aria-label="Search">
                            </form>
                        </li>
                        
                        <li class="nav-item dropdown">
                            @if (User.Identity.IsAuthenticated)
                        {
                            <a class="nav-icon dropdown-toggle d-inline-block d-sm-none" href="#" data-toggle="dropdown">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-settings align-middle"><circle cx="12" cy="12" r="3"></circle><path d="M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z"></path></svg>
                            </a>


                            <a class="nav-link dropdown-toggle d-none d-sm-inline-block" href="#" data-toggle="dropdown">
                                <img src="~/img/profile-img.jpg" class="avatar img-fluid rounded-circle mr-1" alt="Garrett Winters">
                                <span class="text-dark">


                                    @User.Identity.Name
                                    


                                </span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right">
                                <a class="dropdown-item"  asp-controller="account" asp-action="Profile" asp-route-email=@User.Identity.Name>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user feathersmall"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
                                    Profile
                                </a>



                                <a class="dropdown-item" asp-action="Logout" asp-controller="Account">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-log-out feathersmall"><path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path><polyline points="16 17 21 12 16 7"></polyline><line x1="21" y1="12" x2="9" y2="12"></line></svg>Sign out
                                </a>
                            </div>
                            }
                        </li>
                        
                    </ul>
                </div>
            </nav>
            <main class="content">
                <div class="container-fluid p-0">
                    <partial name="_Notifications" />
                    @RenderBody()
                </div>

            </main>
            <div class="modal" tabindex="-1" role="dialog" id="form-modal">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title"></h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">×</span>
                            </button>
                        </div>
                        <div class="modal-body">

                        </div>
                    </div>
                </div>
            </div>

            <div class="loaderbody" id="loaderbody">
                <div class="loader"></div>
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/feather.min.js"></script>
    <script src="~/js/perfect-scrollbar.jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/prism.js"></script>
    <script src="~/js/datatables.js"></script>
 

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
